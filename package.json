{
   "name": "ecomm",
   "version": "1.0.0",
   "type": "module",
   "description": "",
   "main": "index.js",
   "scripts": {
      "dev": "cross-env NODE_ENV=local tsx watch src/index.ts",
      "build": "cross-env NODE_OPTIONS=--max-old-space-size=2048 tsc",
      "start": "cross-env NODE_ENV=dev node dist/index.js",
      "db:generate": "pnpm drizzle-kit generate",
      "db:migrate": "pnpm drizzle-kit migrate",
      "db:push": "pnpm drizzle-kit push",
      "db:studio": "pnpm exec drizzle-kit studio",
      "ba:generate": "npx @better-auth/cli generate --output src/database/auth-schema.ts",
      "ba:migrate": "npx @better-auth/cli migrate",
      "docker:up": "docker compose up -d",
      "docker:down": "docker compose down",
      "n8n:up": "docker compose -f n8n-docker/docker-compose.yml up -d",
      "n8n:down": "docker compose -f n8n-docker/docker-compose.yml down",
      "n8n:logs": "docker compose -f n8n-docker/docker-compose.yml logs -f n8n",
      "n8n:restart": "docker compose -f n8n-docker/docker-compose.yml restart"
   },
   "dependencies": {
      "arktype": "^2.1.22",
      "axios": "^1.12.2",
      "better-auth": "^1.3.24",
      "dotenv": "^17.2.3",
      "drizzle-arktype": "^0.1.3",
      "drizzle-orm": "^0.44.6",
      "hono": "^4.9.9",
      "postgres": "^3.4.7",
      "redis": "^5.9.0"
   },
   "devDependencies": {
      "@hono/node-server": "^1.19.5",
      "@types/node": "^20.11.17",
      "@types/redis": "^4.0.11",
      "@types/supertest": "^6.0.3",
      "cross-env": "^10.1.0",
      "drizzle-kit": "^0.31.5",
      "supertest": "^7.1.4",
      "ts-node": "^10.9.2",
      "tsx": "^4.7.1",
      "typescript": "^5.9.3",
      "vitest": "^3.2.4"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "packageManager": "pnpm@10.18.3"
}
